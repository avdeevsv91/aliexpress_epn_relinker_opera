{
	"manifest_version": 2,
	"author": "Sergey Avdeev",
	"name": "__MSG_name__",
	"short_name": "__MSG_short_name__",
	"description": "__MSG_description__",
	"homepage_url": "https://github.com/avdeevsv91/aliexpress_epn_relinker_opera",
	"version": "1.3.0",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"default_locale": "en",
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"options_page": "settings.html",
	"content_scripts": [
		{
			"js": [
				"jquery-3.4.1.min.js",
				"content.js"
			], 
			"matches": [
				"*://*.aliexpress.com/*"
			], 
			"run_at": "document_idle"
		}
	],
	"permissions": [
		"*://*.aliexpress.com/*"
	]
}
